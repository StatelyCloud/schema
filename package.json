{
  "name": "@stately-cloud/schema",
  "version": "0.2.0",
  "author": "Stately Cloud <support@stately.cloud> (https://stately.cloud/)",
  "description": "Schema language for StatelyDB",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "files": [
    "package.json",
    "README.md",
    "LICENSE.txt",
    "dist/**"
  ],
  "bin": "./dist/cli.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./driver": {
      "types": "./dist/driver.d.ts",
      "default": "./dist/driver.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "start": "node dist/cli.js",
    "fix:prettier": "prettier --write 'src/**/*.ts'",
    "lint:prettier": "prettier 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "fix:eslint": "eslint --fix 'src/**/*.ts'",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stately-cloud/eslint": "workspace:*",
    "@types/jest": "29.5.12",
    "@types/node": "22.4.1",
    "eslint": "9.9.0",
    "jest": "29.7.0",
    "ts-jest": "29.2.4"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.0.0",
    "fast-equals": "^5.0.1",
    "tsx": "^4.7.1",
    "typescript": "^5.5.4"
  }
}
